{
  "name": "auto-console-log",
  "version": "1.0.1",
  "type": "module",
  "author": "Waxer59",
  "description": "autoConsole is a Babel plugin that automatically replaces all calls to console.log() in the code to make debugging and error tracking easier.",
  "main": "./cjs/auto-console-log.js",
  "module": "./esm/auto-console-log.js",
  "exports": {
    ".": {
      "require": "./cjs/auto-console-log.js",
      "default": "./esm/auto-console-log.js"
    }
  },
  "homepage": "https://github.com/Waxer59/auto-console",
  "repository": {
    "type": "git",
    "url": "git://github.com/Waxer59/auto-console.git"
  },
  "scripts": {
    "prepare": "husky install",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "jest __tests__",
    "test:watch": "yarn test --watch",
    "test:manual": "node --watch utils/auto-console-log-manual-testting.js",
    "build": "rollup -c"
  },
  "keywords": [
    "autoConsole",
    "auto console",
    "babel plugin",
    "plugin",
    "babel"
  ],
  "files": [
    "/dist",
    "README.md",
    "LICENSE.md"
  ],
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.21.4",
    "@babel/parser": "7.21.4",
    "@babel/standalone": "7.21.4",
    "@babel/template": "7.20.7",
    "@babel/traverse": "7.21.4",
    "@babel/types": "7.21.4",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-n": "15.0.0",
    "eslint-plugin-promise": "6.0.0",
    "husky": "8.0.0",
    "jest": "29.5.0",
    "lint-staged": "13.2.0",
    "prettier": "2.8.7",
    "rollup": "^3.20.2",
    "rollup-plugin-copy": "^3.4.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
